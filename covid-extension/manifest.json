{
  "manifest_version": 3,
  "name": "Covid Extension",
  "version": "1.0",
  "description": "Shows latest covid data",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Latest Covid Report"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}